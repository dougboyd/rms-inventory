    "passport-google-oauth": "^1.0.0",
    "passport-linkedin-oauth2": "^1.5.0",
    "passport-local": "1.0.0",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "pxpay": "git+https://github.com/sergmetelin/pxpay.git",
    "rabbit.js": "^0.4.4",
    "rand-token": "^0.3.0",
    "reflect-metadata": "^0.1.10",
