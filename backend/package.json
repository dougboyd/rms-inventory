{
  "name": "RealYetiBackend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "Commercial",
  "private": true,
  "scripts": {
    "start": "env TZ='UTC' nodemon bin/www.js --exec babel-node --presets es2015,stage-2",
    "build": "scripts/build.sh",
    "serve": "env TZ='UTC' node dist/www-prod.js",
    "lint": "./node_modules/.bin/eslint ./",
    "test": "NODE_ENV=test mocha $(find src -name '*.test.js') --timeout 5000 --require babel-polyfill --compilers js:babel-register --recursive",
    "coverage": "NODE_ENV=test  ./node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha $(find src -name '*.test.js') --report text --check-coverage -- --timeout 5000 -u tdd --recursive",
    "prepush": "yarn lint && yarn test && cd ../frontend && yarn lint",
    "seed": "./node_modules/.bin/babel-node scripts/seed.js",
    "seed:development": "./node_modules/.bin/sequelize db:seed:undo:all && ./node_modules/.bin/sequelize db:seed:all ",
    "seed:language": "./node_modules/.bin/sequelize db:seed:undo --seed '040 - Language_translations.js' && ./node_modules/.bin/sequelize db:seed --seed '040 - Language_translations.js'",
    "migration": "./node_modules/.bin/sequelize db:migrate",
    "migration-down": "./node_modules/.bin/sequelize db:migrate:undo",
    "migration-down-all": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "language:upsert": "node dist/upsert-language/index.js",
    "language:upsert-dev": "./node_modules/.bin/babel-node bin/upsert-language/index-dev.js",
    "worker-email-dev:start": "env TZ='UTC' nodemon bin/email-worker.js --exec babel-node --presets es2015,stage-2",
    "worker-email:start": "env TZ='UTC' nodemon dist/email-worker-prod.js --exec babel-node --presets es2015,stage-2",
    "worker-email:serve": "env TZ='UTC' node dist/email-worker-prod.js",
    "worker-payments:process": "env TZ='UTC' ./node_modules/.bin/babel-node ./bin/payments-worker.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^1.1.1",
    "@google/maps": "^0.4.2",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.17.2",
    "boom": "^5.1.0",
